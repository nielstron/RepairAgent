Line 401:     * Must be called at least twice:  once with the data to encode, and once
Line 402:     * with inAvail set to "-1" to alert encoder that EOF has been reached,
Line 403:     * so flush last remaining bytes (if not multiple of 3).
Line 404:     * </p><p>
Line 405:     * Thanks to "commons" project in ws.apache.org for the bitwise operations,
Line 406:     * and general approach.
Line 407:     * http://svn.apache.org/repos/asf/webservices/commons/trunk/modules/util/
Line 408:     * </p>
Line 409:     *
Line 410:     * @param in byte[] array of binary data to base64 encode.
Line 411:     * @param inPos Position to start reading data from.
Line 412:     * @param inAvail Amount of bytes available from input for encoding.
Line 413:     */
Line 414:    void encode(byte[] in, int inPos, int inAvail) {
Line 415:        if (eof) {
Line 416:            return;
Line 417:        }
Line 418:        // inAvail < 0 is how we're informed of EOF in the underlying data we're
Line 419:        // encoding.
Line 420:        if (inAvail < 0) {
Line 421:            eof = true;
Line 422:            if (buf == null || buf.length - pos < encodeSize) {
Line 423:                resizeBuf();
Line 424:            }
Line 425:            switch (modulus) {
Line 426:                case 1:
Line 427:                    buf[pos++] = encodeTable[(x >> 2) & MASK_6BITS];
Line 428:                    buf[pos++] = encodeTable[(x << 4) & MASK_6BITS];
Line 429:                    // URL-SAFE skips the padding to further reduce size.
Line 430:                    if (encodeTable == STANDARD_ENCODE_TABLE) {
Line 431:                        buf[pos++] = PAD;
Line 432:                        buf[pos++] = PAD;
Line 433:                    }
Line 434:                    break;
Line 435:
Line 436:                case 2:
Line 437:                    buf[pos++] = encodeTable[(x >> 10) & MASK_6BITS];
Line 438:                    buf[pos++] = encodeTable[(x >> 4) & MASK_6BITS];
Line 439:                    buf[pos++] = encodeTable[(x << 2) & MASK_6BITS];
Line 440:                    // URL-SAFE skips the padding to further reduce size.
Line 441:                    if (encodeTable == STANDARD_ENCODE_TABLE) {
Line 442:                        buf[pos++] = PAD;
Line 443:                    }
Line 444:                    break;
Line 445:            }
Line 446:            if (lineLength > 0) {
Line 447:                System.arraycopy(lineSeparator, 0, buf, pos, lineSeparator.length);
Line 448:                pos += lineSeparator.length;
Line 449:            }
Line 450:        } else {
Line 451:            for (int i = 0; i < inAvail; i++) {
Line 452:                if (buf == null || buf.length - pos < encodeSize) {
Line 453:                    resizeBuf();
Line 454:                }
Line 455:                modulus = (++modulus) % 3;
Line 456:                int b = in[inPos++];
Line 457:                if (b < 0) { b += 256; }
Line 458:                x = (x << 8) + b;
Line 459:                if (0 == modulus) {
Line 460:                    buf[pos++] = encodeTable[(x >> 18) & MASK_6BITS];
Line 461:                    buf[pos++] = encodeTable[(x >> 12) & MASK_6BITS];
Line 462:                    buf[pos++] = encodeTable[(x >> 6) & MASK_6BITS];
Line 463:                    buf[pos++] = encodeTable[x & MASK_6BITS];
Line 464:                    currentLinePos += 4;
Line 465:                    if (lineLength > 0 && lineLength <= currentLinePos) {
Line 466:                        System.arraycopy(lineSeparator, 0, buf, pos, lineSeparator.length);
Line 467:                        pos += lineSeparator.length;
Line 468:                        currentLinePos = 0;
Line 469:                    }
Line 470:                }
Line 471:            }
Line 472:        }
Line 473:    }
